{
  "name": "vue-forum",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "supabase:init": "supabase init",
    "supabase:login": "supabase login",
    "supabase:link": "supabase link --project-ref kftmdpyrecockpoirqys",
    "db:migrate:new": "supabase migration new projects-schema",
    "db:reset": "supabase db reset --linked",
    "db:seed": "node --env-file=.env database/seed.js"
  },
  "dependencies": {
    "@formkit/addons": "^1.6.5",
    "@formkit/vue": "^1.6.5",
    "@supabase/supabase-js": "^2.43.5",
    "@vueuse/core": "^10.10.0",
    "pinia": "^2.1.7",
    "primeicons": "^7.0.0",
    "primevue": "^4.0.0-beta.4",
    "uuid": "^10.0.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@formkit/core": "^1.6.5",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.5",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "supabase": "^1.178.2",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.0",
    "unplugin-vue-router": "^0.10.0",
    "vite": "^5.2.8",
    "vite-plugin-vue-devtools": "^7.0.25",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.11"
  }
}
